{

    "builds": [{

            "src": "BookNest/wsgi.py",

            "use": "@vercel/python",

            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }

        },

        {

            "src": "build.sh",

            "use": "@vercel/static-build",

            "config": { "distDir": "staticfiles_build" }

        }

    ],

    "routes": [{

            "src": "/stati/(.*)",

            "dest": "/static/$1"

        },

        {

            "src": "/(.*)",

            "dest": "BookNest/wsgi.py"

        }

    ]

}